{
    "name": "highsky-portfolio",
    "version": "1.0.0",
    "description": "Highsky의 프리미엄 포트폴리오 웹사이트",
    "main": "index.html",
    "scripts": {
        "lint": "eslint script.js",
        "lint:fix": "eslint script.js --fix",
        "format": "prettier --write \"*.{js,css,html,md}\"",
        "format:check": "prettier --check \"*.{js,css,html,md}\"",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:report": "playwright show-report",
        "test:all": "npm run lint && npm run format:check && npm run test:e2e",
        "serve": "python3 -m http.server 8000"
    },
    "keywords": [
        "portfolio",
        "website",
        "ai",
        "ml",
        "robotics"
    ],
    "author": "Highsky",
    "license": "MIT",
    "devDependencies": {
        "@playwright/test": "^1.40.0",
        "eslint": "^8.55.0",
        "prettier": "^3.1.0"
    }
}